        { "name": "Notes", "type": "textarea", "required": false }
      ]
    },
    {
      "name": "CIIncident",
      "description": "Join object linking CIs to Incident Records - tracks which CIs are affected by incidents.",
      "attributes": [
        { "name": "Incident", "type": "object", "required": true, "referenceObjectType": "IncidentRecord", "cardinality": "single" },
        { "name": "CI Type", "type": "select", "required": true, "allowedValues": ["BusinessService", "Application", "Server", "Database", "NetworkDevice", "StorageDevice"] },
        { "name": "CI", "type": "text", "required": true, "notes": "Reference to affected CI" },
        { "name": "Impact Type", "type": "select", "required": true, "allowedValues": ["Root Cause", "Contributing Factor", "Impacted", "Related"] },
        { "name": "Notes", "type": "textarea", "required": false }
      ]
    }
  ],
